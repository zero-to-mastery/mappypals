{
    "name": "mappypals",
    "version": "0.1.0",
    "homepage": "https://zero-to-mastery.github.io/mappypals/",
    "private": true,
    "devserver": "live-server --browser=Chrome",
    "dependencies": {
        "axios": "^0.18.0",
        "concurrently": "^4.1.0",
        "deck.gl": "^7.0.11",
        "ky": "^0.10.0",
        "mapbox-gl": "^0.53.0",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-map-gl": "^4.0.11",
        "react-map-gl-geocoder": "^2.0.5",
        "react-mapbox-gl": "^4.2.0",
        "react-redux": "^7.1.0",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^3.0.0",
        "redux": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.1.3",
        "tachyons": "^4.11.1"
    },
    "//": {
        "server": "cd server && npm start",
        "serverinstall": "cd server && npm install && npm audit fix",
        "postinstall": "npm audit fix && npm run serverinstall",
        "frontend": "react-scripts start",
        "start": "concurrently \"npm run frontend\" \"npm run server\""
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write \"**/*.{js,css,md}\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "prettier": "1.17.1"
    }
}
